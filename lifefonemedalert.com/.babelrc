{
  "presets": [
    ["@babel/env", {
      "targets": {
        "browsers": [
          "last 2 versions",
          "> 0.5%",
          "not ie <= 10"
        ]
      },
      "corejs": "3.1.3",
      "useBuiltIns": "usage"
    }]
  ],
  "plugins": ["@babel/syntax-dynamic-import", "@babel/plugin-proposal-class-properties"]
}
